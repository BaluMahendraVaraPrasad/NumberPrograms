public class FibRec {
	static int a=0,b=1,c=0;
    public static void main(String[] args) {
		int count=20;
		System.out.println(a+" "+b);
		fib(count-2);
	}
    public static void fib(int count) {
    	if(count>0) {
    		c=a+b;
    		a=b;
    		b=c;
    		System.out.println(""+c);
    		fib(count-1);
    	}    	
    }
}
